/* scroll area */
QScrollArea,
QScrollArea QWidget,
QScrollArea#settingInterface,
#scrollwidget {
    background: transparent;
    border: none;
}

/* header label */
QLabel#settingLabel {
    font-size: 20px;
    font-weight: 600;
    background: transparent;
}

/* LIGHT THEME OVERRIDES */
QWidget[theme="light"] QLabel#settingLabel {
    color: #111;
}

/* DARK THEME OVERRIDES */
QWidget[theme="dark"] QLabel#settingLabel {
    color: #eee;
}
